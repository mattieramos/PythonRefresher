import random

for i in range(3):  # will execute 3 times
    print(random.random())

print('')


for i in range(3):
    print(random.randint(10, 20))  # will print random values between 10 and 20

print('')

members = ['John', 'Mary', 'Bob', 'Matthew']
leader = random.choice(members)  # will print random name from list
print(leader)

print('')

# *** DICE EXERCISE ***


class Dice:
    def roll(self):
        first = random.randint(1, 6)
        second = random.randint(1, 6)
        return first, second


dice = Dice()
print(dice.roll())
